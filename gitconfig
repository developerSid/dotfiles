[core]
   pager = less -F -X
   autocrlf = false
   fileMode = false
   eol = lf
[pager]   
   branch = false
   tag = false
[pull]
   rebase = true
[push]
   default = current
[init]
   defaultBranch = main
[alias]
   lol = log --graph --decorate --pretty=oneline --abbrev-commit
   lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	pu = !git pull $(git remote) $(git branch --show-current)
[color]
   branch = auto
   diff = auto
   interactive = auto
   status = auto
